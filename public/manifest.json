{
  "manifest_version": 3,
  "name": "TruthLens",
  "version": "1.0",
  "description": "A chrome extension designed to analyze potential AI-generated content",
  "permissions": ["activeTab", "storage"],
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  }
}